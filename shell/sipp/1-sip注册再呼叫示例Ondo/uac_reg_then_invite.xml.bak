<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">


<scenario name="UAC with register, then invite (support media)">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send retrans="500">
    <![CDATA[

      REGISTER sip:[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0] <sip:[field0]@[local_ip]:[local_port]>;tag=[call_number]
      To: <sip:[field0]@[remote_ip]:[remote_port]>
      Call-ID: REG///[call_id]
      CSeq: 1 REGISTER
      Contact: sip:[field0]@[local_ip]:[local_port]
      Content-Length: 0
      Expires: 300

    ]]>
  </send>
  <recv response="100" optional="true"/>
  <!--遇到401指令则跳转到带认证信息的注册请求-->
  <recv response="401" auth="true" optional="true" next="1"/>
  <recv response="200" rtd="true" crlf="true" next="2"/>
  <!--标签1是带有认证信息的注册请求，其中field1是认证信息-->
  <label id="1"/>
  <send retrans="500">
    <![CDATA[

      REGISTER sip:[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0] <sip:[field0]@[local_ip]:[local_port]>;tag=[call_number]
      To: <sip:[field0]@[remote_ip]:[remote_port]>
      Call-ID: REG///[call_id]
      CSeq: 2 REGISTER
      Contact: sip:[field0]@[local_ip]:[local_port]
      Expires: 300
      Content-Length: 0
      [field3]
    ]]>
  </send>

  <recv response="100" optional="true"/>
  <recv response="200" rtd="true" crlf="true"/>
  <label id="2"/>
  <send retrans="500">
    <![CDATA[

      INVITE sip:[field2]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0] <sip:[field0]@[local_ip]:[local_port]>;tag=[call_number]
      To: sut <sip:[field2]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: 3 INVITE
      Contact: sip:[field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[local_ip_type] [local_ip]
      t=0 0
      m=audio [auto_media_port] RTP/AVP 8
      a=rtpmap:8 PCMA/8000
    ]]>
  </send>

  <recv response="100" optional="true">
  </recv>

  <recv response="180" optional="true">
  </recv>

  <recv response="183" optional="true">
  </recv>

  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->
  <!-- 可以加入rrs="true" (Record Route Sets)属性,         -->
  <!-- 路由设置会保存在接下来要发送的信息当中         -->
  <!-- 对测试SIP代理和B2BUA是非常有用的         -->
  <recv response="200" rtd="true" crlf="true" rrs="true">
    <!-- Play a pre-recorded PCAP file (RTP stream)                       -->
  </recv>

  <pause milliseconds="1000"/>

  <!--z9hG4bKkjshdyff-->
  <send>
    <![CDATA[

      ACK sip:[field2]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch-2]
      From: [field0] <sip:[field0]@[local_ip]:[local_port]>;tag=[call_number]
      To: sut <sip:[field2]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 3 ACK
      Contact: sip:[field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
    <action>
      <exec play_pcap_audio="song-711a.pcap"/>
    </action>
  </send>

  <pause milliseconds="20000"/>

  <!-- The 'crlf' option inserts a blank line in the statistics report. -->
  <send retrans="500">
    <![CDATA[

      BYE [next_url] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0] <sip:[field0]@[local_ip]:[local_port]>;tag=[call_number]
      To: sut <sip:[field2]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 5 BYE
      [routes]
      Contact: sip:[field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <recv response="100" optional="true">
  </recv>

  <recv response="200" crlf="true">
  </recv>

  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>

